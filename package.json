{
  "name": "kento-client",
  "version": "1.0.0",
  "description": "",
  "main": "src/js/index.js",
  "scripts": {
    "watch:stylesheets": "node-sass -w src/stylesheets/kento_client.scss -o dist/stylesheets",
    "watch:javascripts": "NODE_ENV=development webpack --watch",
    "watch": "npm-run-all --parallel watch:*",
    "clean": "rm -rf dist/stylesheets/* dist/javascripts/bundle.min.js",
    "build:stylesheets": "node-sass src/stylesheets/kento_client.scss -o dist/stylesheets",
    "build:javascripts_prod": "NODE_ENV=production webpack -p",
    "build:javascripts_dev": "NODE_ENV=development webpack",
    "build": "npm-run-all clean --parallel build:*",
    "build-mt:development": "NODE_ENV=development webpack --config webpack.config.mt.js",
    "build-mt:production": "NODE_ENV=production webpack --config webpack.config.mt.js",
    "build-mt": "npm-run-all --parallel build-mt:*",
    "watch-mt": "NODE_ENV=development webpack --config webpack.config.mt.js --watch",
    "start": "webpack-dev-server --config webpack.dev-server.config.js --progress --inline --colors",
    "test": "NODE_ENV=test NODE_PATH=app/assets/javascripts/src mocha --compilers js:babel-register $(find app/assets/javascripts/test -name '*.spec.js') --require app/assets/javascripts/test/global.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "test-storybook": "storyshots",
    "build-storybook": "build-storybook"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storyshots": "^2.1.0",
    "assets-webpack-plugin": "^3.3.0",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.18.0",
    "bootstrap-sass": "^3.3.6",
    "flow-bin": "^0.33.0",
    "html-webpack-plugin": "^2.24.0",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "nock": "^7.2.2",
    "node-sass": "^3.7.0",
    "npm-run-all": "^3.1.1",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.3",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "doc-ready": "^1.0.3",
    "js-cookie": "^2.1.0",
    "json-kifu-format": "^1.0.4",
    "material-ui": "^0.16.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "private": true
}
